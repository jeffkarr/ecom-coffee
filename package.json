{
  "name": "ecom-coffee",
  "version": "0.1.0",
  "description": "This eCommerce app uses React.js and Redux.js libraries with an Express server.",
  "main": "server.js",
  "author": "Jeff Karr",
  "license": "MIT",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "^1.18.9",
    "react": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffkarr/ecom-coffee.git"
  },
  "bugs": {
    "url": "https://github.com/jeffkarr/ecom-coffee/issues"
  },
  "homepage": "https://github.com/jeffkarr/ecom-coffee#readme"
}
